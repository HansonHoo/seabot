<launch>

    <node pkg="rosbag" type="record" name="record" output="screen" args="--all" respawn="true" respawn_delay="5"/>

    <include file="$(find seabot)/launch/seabot_driver.launch"/>

    <include file="$(find fusion)/launch/depth_filter.launch"/>
    <include file="$(find fusion)/launch/lambert.launch"/>

    <node pkg="diagnostic_aggregator" type="aggregator_node" name="diag_agg" >
        <rosparam command="load" file="$(find seabot)/diagnostic/seabot_analyzers.yaml" />
    </node>

</launch>