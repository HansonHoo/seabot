<launch>

    <include file="$(find seabot)/launch/base.launch"/>

    <node name="sliding_node" pkg="seabot_depth_regulation" type="sliding_node" output="screen" ns="regulation" respawn="true" respawn_delay="5">
        <param name="g" value="9.81" />
        <param name="rho_eau" value="1020.0" />
        <param name="m" value="8.810" />
        <param name="C_f" value="0.1" />
        <param name="tick_to_volume" value="1.431715402026599e-07" />

        <param name="frequency" value="1.0" />
        <param name="offset" value="600" />
        <param name="hysteresis_piston" value="0.6" />

        <param name="K_factor" value="1.0" />
        <param name="K_velocity" value="300.0" />
        <param name="K_acc" value="100.0" />

        <param name="piston_position_min" value="0.0" />
        <param name="piston_position_max" value="1250.0" />

        <param name="compression_tick_factor" value="30.0" />

    </node>

<!-- Kf : 10, Kv: 50, freq:1, offset = 865 / 964 -->

    <node name="regulation" pkg="seabot_depth_regulation" type="regulation_talker.py" output="screen" respawn="false">
    </node>

</launch>

